{
    "name": "cnizzardini/cakephp-sakila-plugin",
    "description": "Automatically generate Swagger documentation from your exist routes with minimal comments and configuration",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": ["cakephp swagger","cakephp4 swagger","cake swagger"],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sakila\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sakila\\Test\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/cnizzardini/cakephp-sakila-plugin/issues",
        "wiki": "https://github.com/cnizzardini/cakephp-sakila-plugin/wiki",
        "source": "https://github.com/cnizzardini/cakephp-sakila-plugin"
    },
    "authors": [
        {
            "name": "Chris Nizzardini",
            "role": "Developer"
        }
    ]
}
